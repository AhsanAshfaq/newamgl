<odoo>
    <data>
        <record id="amgl.review_complete_withdraw_users_form" model="ir.ui.view">
            <field name="name">Vault Authorizers</field>
            <field name="model">amgl.review_users</field>
            <field name="arch" type="xml">
                <form string="Users">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="amgl.review_complete_withdraw_users_tree" model="ir.ui.view">
            <field name="name">Vault Authorizers</field>
            <field name="model">amgl.review_users</field>
            <field name="arch" type="xml">
                <tree string="Users">
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>